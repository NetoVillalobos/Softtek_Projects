@{
    ViewBag.Title = "Home Page";
}


<div class="row">
    <div class="col-md-4">
        <h2>Getting started</h2>
        <p>
           <input type="text" class="form-control" id="txtkeyword" placeholder="buscar"/>
        </p>
        <p><a class="btn btn-default" href="javascript:void(0);" onclick="btnSearch();">Buscar</a></p>
    
        <div class="container" id="ListYoutube">

        </div>
    </div>
    <div class="col-md-4">
        <div id="player"></div>

        <div id="PruebaPlayList"></div>
    </div>

</div>

<script>
    function btnSearch() {
        $.ajax(
            {
                url: '@Url.Action("SearchList")?keyword='+$('#txtkeyword').val(),
                type: 'GET',
                success: function (result)
                {
                    //response
                    $('#ListYoutube').html(result);
                }
            });
    }
</script>